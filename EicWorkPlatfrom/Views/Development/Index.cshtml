@{
    Layout = null;
}

<!DOCTYPE html>

<html ng-app="bpm.developmentApp">
<head>
    <meta http-equiv="Content-Type" />
    <meta name="viewport" content="width=device-width" />
    <title>研发管理</title>
    <style>
        .bkcolor {
            background-color: #87CEFA;
        }
    </style>
    <link rel="shortcut icon" href="@Url.Content("~/Content/login/lmlogo.png")" />

    <link href="@Url.Content("~/Content/bootstrap/dist/css/bootstrap.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootwatch/cerulean.bootstrap.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/font-awesome/css/font-awesome.min.css")" type="text/css" rel="stylesheet" />
    <link href="@Url.Content("~/Content/main.css")" type="text/css" rel="stylesheet" />

    <link href="@Url.Content("~/Content/zTree/dist/css/zTreeStyle/zTreeStyle.css")" type="text/css" rel="stylesheet" />

    @Styles.Render("~/css/anstrap")
</head>
<body>

    @if (ViewBag.IsLoginMode)
    {
        <div yl-navigation-layout data-module-text="研发管理" data-module-name="DevelopmentManage"></div>
    }
    else
    { 
        <div>
            <header role="banner">
                <nav role="navigation" class="navbar navbar-static-top navbar-inverse" ng-cloak>
                    <div class="container">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                                <span>
                                    <img alt="光圣科技（宁波）有限公司" title="光圣科技（宁波）有限公司"
                                         src="@Url.Content("~/Content/login/lmlogo.png")"
                                         style="margin-left: -50px;margin-top: -5px;background-color: whitesmoke;height: 28px;width: 32px;border-radius: 5px;margin-right: 25px;" />
                                    EIC <span style="font-size:8px">(V01.012)</span>
                                </span>
                            </a>
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="navbar-collapse collapse">
                            <ul class="nav navbar-nav">
                                <li>
                                    <a href="#">生产管理</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </nav>
            </header>

            <main class="container-fluid">
                <div class="row" ng-cloak>
                    <div class="pull-left" style="width:{{navLayout.navLeftSize}};" ng-show="navLayout.navViewSwitch">
                        <div class="bs-callout bs-callout-info">
                            <h4>
                                <span class="fa fa-list-ul">导航菜单</span>
                                <span class="pull-right">
                                    <span class="badge" ng-click="navLayout.switchView()" title="点击折叠导航面板" style="cursor:pointer"><i class="fa fa-hand-o-left"></i></span>
                                </span>
                            </h4>
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <ol class="rounded-list">
                                    <li>
                                        <a data-ui-sref-active="active" data-ui-sref="create8DForm" style="cursor:pointer">创建8D表单</a>
                                        <a data-ui-sref-active="active" data-ui-sref="handle8DFolw" style="cursor:pointer">报告登记</a>
                                        <a data-ui-sref-active="active" data-ui-sref="close8DForm" style="cursor:pointer">结案签核</a>


                                        <a data-ui-sref-active="active" data-ui-sref="iqcInspectionModeConfiguration" style="cursor:pointer">进料检验IQC方式配置</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionModeSwitchConfiguration" style="cursor:pointer">检验方式转换配置</a>

                                        <a data-ui-sref-active="active" data-ui-sref="iqcInspectionItemConfiguration" style="cursor:pointer">进料检验IQC项目配置</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionDataGatheringOfIQC" style="cursor:pointer">IQC检验数据采集</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionFormManageOfIqc" style="cursor:pointer">IQC检验单管理</a>

                                        <a data-ui-sref-active="active" data-ui-sref="fqcInspectionItemConfiguration" style="cursor:pointer">FQC检验项目配置</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionDataGatheringOfFQC" style="cursor:pointer">FQC检验数据采集</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionFormManageOfFqc" style="cursor:pointer">FQC检验单管理</a>

                                        <a data-ui-sref-active="active" data-ui-sref="ipqcInspectionItemConfiguration" style="cursor:pointer">IPQC检验项目配置</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionDataGatheringOfIPQC" style="cursor:pointer">IPQC检验数据采集</a>
                                        <a data-ui-sref-active="active" data-ui-sref="inspectionFormManageOfIpqc" style="cursor:pointer">IPQC检验单管理</a>
                                    </li>
                                </ol>
                            </div>
                        </div>
                    </div>
                    <div class="pull-left" style="width:{{navLayout.navLeftSize}};" ng-hide="navLayout.navViewSwitch">
                        <span class="badge" ng-click="navLayout.switchView()" title="点击打开导航面板" style="cursor:pointer"><i class="fa fa-hand-o-right"></i></span>
                    </div>
                    <div class="pull-right" style="width:{{navLayout.navMainSize}};">
                        <div data-ui-view>

                        </div>
                    </div>
                </div>
            </main>
        </div>
    }

    @Scripts.Render("~/bundles/bootstrap")

    @Scripts.Render("~/bundles/angularJs")

    @Scripts.Render("~/bundles/anstrap")

    <script src="@Url.Content("~/Content/ztree/dist/js/jquery.ztree.all.min.js")"></script>

    <script src="@Url.Content("~/Scripts/controller/masterProduct/mpConfigApp.js")"></script>

    <script src="@Url.Content("~/Scripts/common/angulee.js")"></script>
    <script src="@Url.Content("~/Scripts/common/commDirective.js")"></script>

    <script src="@Url.Content("~/Scripts/controller/development/developmentApp.js")"></script>
 


</body>
</html>