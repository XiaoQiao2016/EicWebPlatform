<div class="panel panel-default" ng-controller="astArchiveScreeningCtrl">
    <div bs-tabs bs-active-pane="vmManager.activeTab" cg-busy="{promise:promise}">
        <div bs-pane data-title="设备档案总览" data-name="initTab">
