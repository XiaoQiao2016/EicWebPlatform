<div class="panel panel-default" ng-controller="astArchiveOverviewCtrl">
    <div bs-tabs bs-active-pane="vmManager.activeTab" cg-busy="{promise:promise}">
        <div bs-pane data-title="基本信息" data-name="baseInfoTab">
            <div class="well">
                <div class="table-responsive">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr class="info">
                                <th>财产编号</th>
                                <th>设备名称</th>
                                <th>设备型号</th>
                                <th>制造编号</th>
                                <th>资产类别</th>
                                <th>设备类别</th>
                                <th>税务类别</th>
                                <th>单位</th>
                                <th>生产厂家</th>
                                <th>购入日期</th>
                                <th>保管人工号</th>
                                <th>操作(<i class="fa fa-pencil-square-o"></i>)</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="item in vmManager.datasource track by $index">
                                <td>{{item.AssetNumber}}</td>
                                <td>{{item.EquipmentName}}</td>
                                <td>{{item.EquipmentSpec}}</td>
                                <td>{{item.ManufacturingNumber}}</td>
                                <td>{{item.AssetType}}</td>
                                <td>{{item.EquipmentType}}</td>
                                <td>{{item.TaxType}}</td>
                                <td>{{item.Unit}}</td>
                                <td>{{item.Manufacturer}}</td>
                                <td>{{item.DeliveryDate}}</td>
                                <td>{{item.SafekeepUser}}</td>
                                <td>{{item.SafekeepDepartment}}</td>
                                <td>
                                    <button class="btn btn-xs btn-primary" ng-click="operate.editItem(item)" title="明细">
                                        明细
                                    </button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div bs-pane data-title="校验信息" data-name="checkInfoTab">
            <div class="well" cg-busy="{promise:searchPromise}">
                <div class="table-responsive">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr class="info">
                                <th>财产编号</th>
                                <th>设备名称</th>
                                <th>设备型号</th>
                                <th>制造编号</th>
                                <th>是否校验</th>
                                <th>校验类型</th>
                                <th>校验日期</th>
                                <th>校验间隔</th>
                                <th>计划校验日期</th>
                                <th>校验状态</th>
                                <th>操作(<i class="fa fa-pencil-square-o"></i>)</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="item in vmManager.datasource track by $index">
                                <td>{{item.AssetNumber}}</td>
                                <td>{{item.EquipmentName}}</td>
                                <td>{{item.EquipmentSpec}}</td>
                                <td>{{item.FunctionDescription}}</td>
                                <td>{{item.ServiceLife}}</td>
                                <td>{{item.EquipmentPhoto}}</td>
                                <td>{{item.AssetType}}</td>
                                <td>{{item.EquipmentType}}</td>
                                <td>{{item.TaxType}}</td>
                                <td>{{item.Unit}}</td>
                                <td>{{item.Manufacturer}}</td>
                                <td>{{item.ManufacturingNumber}}</td>
                                <td>{{item.ManufacturerWebsite}}</td>
                                <td>{{item.ManufacturerTel}}</td>
                                <td>{{item.AfterSalesTel}}</td>
                                <td>{{item.AddMode}}</td>
                                <td>{{item.DeliveryDate}}</td>
                                <td>{{item.DeliveryUser}}</td>
                                <td>{{item.DeliveryCheckUser}}</td>
                                <td>{{item.SafekeepWorkerID}}</td>
                                <td>{{item.SafekeepUser}}</td>
                                <td>{{item.SafekeepDepartment}}</td>
                                <td>{{item.Installationlocation}}</td>
                                <td>{{item.IsMaintenance}}</td>
                                <td>{{item.MaintenanceDate}}</td>
                                <td>{{item.MaintenanceInterval}}</td>
                                <td>{{item.PlannedMaintenanceDate}}</td>
                                <td>{{item.MaintenanceState}}</td>
                                <td>{{item.IsCheck}}</td>
                                <td>{{item.CheckType}}</td>
                                <td>{{item.CheckDate}}</td>
                                <td>{{item.CheckInterval}}</td>
                                <td>{{item.PlannedCheckDate}}</td>
                                <td>{{item.CheckState}}</td>
                                <td>{{item.State}}</td>
                                <td>{{item.IsScrapped}}</td>
                                <td>{{item.InputDate}}</td>
                                <td>{{item.OpDate}}</td>
                                <td>{{item.OpTime}}</td>
                                <td>{{item.OpPerson}}</td>
                                <td>{{item.OpSign}}</td>
                                <td>{{item.Id_Key}}</td>
                                <td>
                                    <button class="btn btn-xs btn-primary" ng-click="operate.editItem(item)" title="修改">
                                        修改
                                    </button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div bs-pane data-title="保养信息" data-name="maintenanceInfoTab">
            <div class="well">
                <div class="table-responsive">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr class="info">
                                <th>财产编号</th>
                                <th>设备名称</th>
                                <th>设备型号</th>
                                <th>制造编号</th>
                                <th>是否保养</th>
                                <th>保养日期</th>
                                <th>保养间隔</th>
                                <th>计划保养日期</th>
                                <th>保养状态</th>
                                <th>操作(<i class="fa fa-pencil-square-o"></i>)</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="item in vmManager.datasource track by $index">
                                <td>{{item.AssetNumber}}</td>
                                <td>{{item.EquipmentName}}</td>
                                <td>{{item.EquipmentSpec}}</td>
                                <td>{{item.ManufacturingNumber}}</td>
                                <td>{{item.IsMaintenance}}</td>
                                <td>{{item.MaintenanceDate}}</td>
                                <td>{{item.MaintenanceInterval}}</td>
                                <td>{{item.PlannedMaintenanceDate}}</td>
                                <td>{{item.MaintenanceState}}</td>
                                <td>
                                    <button class="btn btn-xs btn-primary" ng-click="operate.editItem(item)" title="履历">
                                        履历
                                    </button>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div bs-pane data-title="维修信息" data-name="repairInfoTab">

        </div>
        <div bs-pane data-title="报废信息" data-name="scrapInfoTab">

        </div>
    </div>
    <div class="panel-footer">

    </div>
</div>