<div class="panel panel-primary" ng-controller="supplierGatherInfoCtrl">
    <div class="panel-heading">
        <h6 class="panel-title">供应商档案信息总览</h6>
    </div>
    <div class="panel-body">
        <div class="pull-left">
            <label>供应商Id:</label>
            <input type="text" ng-model="vmManager.supplierId" placeholder="供应商ID"  />
            <button  class="btn btn-success btn-sm"  ng-click="vmManager.searchSupplierDatas()" >查询<i class="fa fa-search"></i></button>
        </div>
        <div class="pull-right">
            <button class="btn btn-success btn-sm">供应商档案信息导出</button>
        </div>
        <div class="clearfix"></div>
        <div class="h-split"></div>
        <div id="equipmentDataWindow">
            <div bs-tabs bs-active-pane="vmManager.activeTab" cg-busy="{promise:promise}">
                @*供应商基础信息*@
                <div bs-pane data-title="供应商信息" data-name="baseInfoTab">
                    <div class="well">
                        <div class="table-responsive">
                            <table class="table table-condensed table-bordered table-hover" style="table-layout:fixed">
                                <colgroup>
                                    @*编号  *@
                                    <col width="30" />
                                    @* 采购类型 *@
                                    <col width="50" />
                                    @* 供应商类型 *@
                                    <col width="60" />
                                    @* 简称 *@
                                    <col width="100" />
                                    @* 全称 *@
                                    <col width="200" />
                                    @* 联系人 *@
                                    <col width="50" />
                                    @* 地址 *@
                                    <col width="150" />
                                    @* 付款方式 *@
                                    <col width="50" />
                                    @*负责人  *@
                                    <col width="40" />
                                    @* 备注 *@
                                    <col width="30" />
                                    @* 操作 *@
                                    <col width="30" />
                                </colgroup>
                                <thead>
                                    <tr class="info">
                                        <th>编号</th>
                                        <th>采购类型</th>
                                        <th>供应商类型</th>
                                        <th>简称</th>
                                        <th>全称</th>
                                        <th>联系人</th>
                                        <th>地址</th>
                                        <th>付款方式</th>
                                        <th>负责人</th>
                                        <th>备注</th>
                                        <th>操作<i></i></th>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="item in vmManager.SupplierInfoDatas track by $index">
                                        <td>{{item.SupplierId}}</td>
                                        <td>{{item.SupplierType}}</td>
                                        <td>{{item.SupplierProperty}}</td>
                                        <td>{{item.SupplierShortName}}</td>
                                        <td>{{item.SupplierName}}</td>
                                        <td>{{item.SupplierUser}}</td>
                                        <td>{{item.SupplierAddress}}</td>
                                        <td>{{item.PayCondition}}</td>
                                        <td>{{item.Remark}}</td>
                                        <td>{{item.DeliveryDate}}</td>
                                    </tr>
                                </tbody>
                            </table>
                            <div yl-pagination
                                 data-datasource="vmManager.datasource"
                                 data-dataset="vmManager.datasets">
                            </div>
                        </div>
                    </div>
                </div>
                @* 供应商证信息 *@
                <div bs-pane data-title="证书信息" data-name="maintenanceInfoTab">
                    <div class="well">
                        <div class="table-responsive">
                            <table class="table table-condensed table-bordered table-hover">
                                <thead>
                                    <tr class="info">
                                        <th>编号</th>
                                        <th>证书名称</th>
                                        <th>文件类型</th>
                                        <th>获取证书日期</th>
                                        <th>文件名称</th>
                                        <th>上传人</th>
                                        <th>上传时间</th>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="item in vmManager.supplierEligibleCertificateDatas track by $index">
                                        <td>{{item.SupplierId}}</td>
                                        <td>{{item.EligibleCertificate}}</td>
                                        <td>{{item.DateOfCertificate}}</td>
                                        <td>{{item.CertificateFileName}}</td>
                                        <td>{{item.OpPerson}}</td>
                                        <td>{{item.OpDate}}</td>
                                    </tr>
                                </tbody>
                            </table>
                            <div yl-pagination
                                 data-datasource="vmManager.supplierEligibleCertificateDatas"
                                 data-dataset="vmManager.supplierEligibleCertificateDatas">
                            </div>
                        </div>
                    </div>
                </div>
                @* 考核信息 *@
                <div bs-pane data-title="考核信息" data-name="repairInfoTab">
                    <div class="well">
                        <div role="tabpanel" class="tab-pane active" id="Repaired">
                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr class="info">
                                            <th>编号</th>
                                            <th>全称</th>
                                            <th>供应商类型</th>
                                            <th>采购类型</th>
                                            <th>地址</th>
                                            <th>联系人</th>
                                            <th>最近采购时间</th>
                                            <th>负责人</th>
                                            <th>备注</th>
                                            <th>操作<i></i></th>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="item in vmManager.datasets track by $index">
                                            <td>{{item.AssetNumber}}</td>
                                            <td>{{item.EquipmentName}}</td>
                                            <td>{{item.EquipmentSpec}}</td>
                                            <td>{{item.ManufacturingNumber}}</td>
                                            <td>{{item.AssetType}}</td>
                                            <td>{{item.EquipmentType}}</td>
                                            <td>{{item.TaxType}}</td>
                                            <td>{{item.Unit}}</td>
                                            <td>{{item.Manufacturer}}</td>
                                            <td>{{item.DeliveryDate}}</td>
                                            <td>{{item.SafekeepUser}}</td>
                                            <td>{{item.SafekeepDepartment}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div yl-pagination data-dataset="vmManager.repairDataSets" data-datasource="vmManager.repairDataSource"></div>
                            </div>
                        </div>
                    </div>
                </div>
                @* 辅导信息 *@
                <div bs-pane data-title="辅导信息" data-name="scrapInfoTab">
                   <div class="well">
                       <div role="tabpanel" class="tab-pane active" id="Repaired">
                           <div class="table-responsive">
                               <table class="table table-striped table-hover">
                                   <thead>
                                       <tr class="info">
                                           <th>编号</th>
                                           <th>全称</th>
                                           <th>供应商类型</th>
                                           <th>采购类型</th>
                                           <th>地址</th>
                                           <th>联系人</th>
                                           <th>最近采购时间</th>
                                           <th>负责人</th>
                                           <th>备注</th>
                                           <th>操作<i></i></th>
                                   </thead>
                                   <tbody>
                                       <tr ng-repeat="item in vmManager.datasets track by $index">
                                           <td>{{item.AssetNumber}}</td>
                                           <td>{{item.EquipmentName}}</td>
                                           <td>{{item.EquipmentSpec}}</td>
                                           <td>{{item.ManufacturingNumber}}</td>
                                           <td>{{item.AssetType}}</td>
                                           <td>{{item.EquipmentType}}</td>
                                           <td>{{item.TaxType}}</td>
                                           <td>{{item.Unit}}</td>
                                           <td>{{item.Manufacturer}}</td>
                                           <td>{{item.DeliveryDate}}</td>
                                           <td>{{item.SafekeepUser}}</td>
                                           <td>{{item.SafekeepDepartment}}</td>
                                       </tr>
                                   </tbody>
                               </table>
                               <div yl-pagination ng-if="vmManager.discardDataSource.length>0"
                                    data-dataset="vmManager.discardDataSets" data-datasource="vmManager.discardDataSource"></div>
                           </div>
                       </div>
                    </div>
                </div>
                @* 稽核信息 *@
                <div bs-pane data-title="稽核信息" data-name="supGradeInfoTab">
                    <div role="tabpanel" class="tab-pane active" id="Repaired">
                        <dir class="well">
                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr class="info">
                                            <th>编号</th>
                                            <th>全称</th>
                                            <th>供应商类型</th>
                                            <th>采购类型</th>
                                            <th>地址</th>
                                            <th>联系人</th>
                                            <th>最近采购时间</th>
                                            <th>负责人</th>
                                            <th>备注</th>
                                            <th>操作<i></i></th>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="item in vmManager.datasets track by $index">
                                            <td>{{item.AssetNumber}}</td>
                                            <td>{{item.EquipmentName}}</td>
                                            <td>{{item.EquipmentSpec}}</td>
                                            <td>{{item.ManufacturingNumber}}</td>
                                            <td>{{item.AssetType}}</td>
                                            <td>{{item.EquipmentType}}</td>
                                            <td>{{item.TaxType}}</td>
                                            <td>{{item.Unit}}</td>
                                            <td>{{item.Manufacturer}}</td>
                                            <td>{{item.DeliveryDate}}</td>
                                            <td>{{item.SafekeepUser}}</td>
                                            <td>{{item.SafekeepDepartment}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div yl-pagination ng-if="vmManager.discardDataSource.length>0"
                                     data-dataset="vmManager.discardDataSets" data-datasource="vmManager.discardDataSource"></div>
                            </div>
                        </dir>
                    </div>

                </div>
            </div>
        </div>
    </div>




    <div class="panel-footer">

    </div>
</div>