<span ng-show="titleVisible">选择年份</span>
<div class="btn-group">
    <div class="btn btn-primary">
        <i class="fa fa-arrow-circle-o-up" ng-click="upYear()">
        </i>
    </div>
    <button type="button" class="btn btn-primary"
            data-animation="am-flip-x"
            bs-dropdown aria-haspopup="true" aria-expanded="false">
        <span style="border-bottom:1px dotted white;">
            {{currentYear}}
        </span>
    </button>
    <div class="btn btn-primary">
        <i class="fa fa-arrow-circle-o-down" ng-click="downYear()"></i>
    </div>
</div>
