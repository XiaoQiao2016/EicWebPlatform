                                           bs-options="item.ProductName as item.ProductName for item in vmManager.erpOrderInfoDatasSet" limit="10" bs-typeahead />
                                    <div class="input-group-addon">
                                        <i class="fa fa-search" ng-click="vmManager.confirmSearch($event,filterErpOrder)"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="h-split"></div>
                        </div>
                        <div class="row">
                            <div style="height:540px;overflow-y:auto;">
                                <table class="table table-condensed table-bordered table-hover">
                                    <thead>
                                        <tr class="info">
                                            <th>工单单号</th>
                                            <th>产品品名</th>
                                            <th>生产数量</th>
                                            <th>入库数量</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="item in vmManager.erpOrderInfoDatasSet|filter:filterErpOrder ">
