<div class="panel panel-primary" ng-controller="recetionExpressCtrl">
    <div class="panel-heading">
        <h6 class="panel-title">快递输录入表</h6>
    </div>
    <div class="DivInput">
        <div class="panel-body">
            <div bs-pane data-title="快递输录入登记" data-name="initTab">
                <div class="row">
                    <div class="col-md-8">
                        <div class="well">
                            <form role="form" name="formrecetionExpress" novalidate class="form-horizontal" onkeypress="if (event.keyCode === 13) return false;">
                                <fieldset>

                                    <div class="form-group">
                                        <label for="sdId" class="col-md-2 control-label">快递单号:</label>
                                        <div class="col-md-4">
                                            <input name="sdId" id="Text1" type="text" class="form-control" ng-model="vm.ExpressId" />
                                        </div>
                                        <label for="expectedProductionNumber" class="col-md-2 control-label">快递公司:</label>
                                        <div class="col-md-4">
                                            <input name="expectedProductionNumber" type="text" class="form-control" ng-model="vm.ExpressCompany" />
                                        </div>

                                    </div>

                                    <div class="form-group">
                                        <label for="triggerDevUnit" class="col-md-2 control-label">发货地址:</label>
                                        <div class="col-md-4">
                                            <input name="triggerDevUnit" id="Text3" type="text" class="form-control" ng-model="vm.SendGoodsCompanyAddress" />
                                        </div>
                                        <label for="fillInPreparer" class="col-md-2 control-label">发货件数:</label>
                                        <div class="col-md-4">
                                            <input name="fillInPreparer" id="Text4" type="text" rows="2" class="form-control" ng-model="vm.GoodsNumber" />
                                        </div>
                                    
                                    </div>
                                    <div class="form-group">
                                        <label for="fillInCheckPreparer" class="col-md-2 control-label">签收人:</label>
                                        <div class="col-md-4">
                                            <input name="fillInCheckPreparer" type="text" class="form-control" ng-model="vm.ReceiverName" />
                                        </div>
                                       
                                        <label for="sdCustomer" class="col-md-2 control-label">收货人:</label>
                                        <div class="col-md-4">
                                            <input name="functionDescription" id="Text2" type="text" class="form-control" ng-model="vm.GetGoodsPerson" />
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label for="sdPutInDate" class="col-md-2 control-label">收件日期:</label>
                                        <div class="col-md-4">
                                            <input name="sdPutInDate" type="date" class="form-control" ng-model="vm.GetGoodsDate" />
                                        </div>
                                        <label for="expectedProductionNumber" class="col-md-2 control-label">取件人:</label>
                                        <div class="col-md-4">
                                            <input name="expectedProductionNumber" type="text" class="form-control" ng-model="vm.Consignee" />
                                        </div>
                                    </div>
                                </fieldset>
                                <hr />
                                <div class="alert alert-info">
                                    <div class="row">
                                        <div class="col-md-4">
                                        </div>
                                        <div class="col-md-6">
                                            <button class="btn btn-primary" ng-click="operate.saveAll(formrecetionExpress.$valid)"><i class="fa fa-save"></i>保存</button>
                                            <button class="btn btn-default" ng-click="operate.refresh()"><i class="fa fa-undo"></i>取消</button>
                                            <span yl-operatemsg-board></span>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <ul class="list-group">
                            <li class="list-group-item active">
                                <div class="row">
                                    <div class="col-md-4">
                                        <h4 class="list-title">
                                            <i class="fa fa-list"></i>
                                            <span>列表</span>
                                        </h4>
                                    </div>
                                    <div class="col-md-8">
                                        <div class="input-group">
                                            <input class="form-control" type="text" />
                                            <div class="input-group-addon">
                                                <i class="fa fa-search"></i>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            <li class="list-group-item" ng-click="vmManager.selectDevelopmentOrder(item)"
                                data-title="设计编号:{{item.DevelopmentRmId}}" data-bs-tooltip
                                ng-repeat="item in vmManager.DevelopmentRmIds track by $index">
                                编号：
                                <span class="label label-success" style="cursor:pointer">{{item.RmId}}</span>
                                -----
                                设计产品名称：
                                <span>
                                    <i class="fa fa-envelope-o"></i>
                                    {{item.ProductName}}
                                </span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-footer" />
    </div>
</div>